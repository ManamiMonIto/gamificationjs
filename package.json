    "eslint": "^4.19.1",
    "nodemon": "^1.17.3",
    "pg": "^7.12.1"
  },
  "dependencies": {
    "@koa/cors": "2",
    "bcryptjs": "^2.4.3",
    "bookshelf": "^0.13.3",
    "bookshelf-secure-password": "^3.1.0",
    "jsonwebtoken": "^8.2.1",
    "knex": "^0.19.2",
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-helmet": "^4.0.0",
    "koa-morgan": "^1.0.1",
